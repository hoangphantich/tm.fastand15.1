<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nguyentiendung.mediaplayer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />

    <application
        android:icon="@drawable/ic_launcher_2"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.nguyentiendung.mediaplayer.MainPlayer"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAINplayer" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="com.nguyentiendung.service.MusicService"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.nguyentiendung.mediaplayer.TOGGLE_PLAYBACK" />
                <action android:name="com.nguyentiendung.mediaplayer.PLAY" />
                <action android:name="com.nguyentiendung.mediaplayer.PREVIOUS" />
                <action android:name="com.nguyentiendung.mediaplayer.NEXT" />
                <action android:name="com.nguyentiendung.mediaplayer.PAUSE" />
                <action android:name="ccom.nguyentiendung.mediaplayer.SEEK" />
                <action android:name="com.nguyentiendung.mediaplayer.SUPPLY_PLAYLIST" />
                <action android:name="com.nguyentiendung.mediaplayer.PLAY_SPECIFIC_SONG" />
                <action android:name="com.nguyentiendung.mediaplayer.REQUEST_STATUS" />
                <action android:name="com.nguyentiendung.mediaplayer.TOGGLE_REPEATMODE" />
                <action android:name="com.nguyentiendung.mediaplayer.TOGGLE_SHUFFLE" />
                <action android:name="com.nguyentiendung.mediaplayer.REORDER_PLAYLIST" />
                <action android:name="com.nguyentiendung.mediaplayer.SETUP_AS_FOREGROUND" />
            </intent-filter>
        </service>

        <receiver android:name="com.nguyentiendung.receiver.MusicIntentReceiver" >
            <intent-filter>
                <action android:name="android.media.AUDIO_BECOMING_NOISY" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.nguyentiendung.mediaplayer.SplashActivity"
            android:theme="@android:style/Theme.Holo.Dialog.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nguyentiendung.mediaplayer.VidduActivity"
            android:label="@string/title_activity_viddu" >
            <intent-filter>
                <action android:name="android.intent.action.MAINe" />

                <category android:name="android.intent.category.LAUNCHERe" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nguyentiendung.mediaplayer.Game_Caro"
            android:label="@string/title_activity_tic_tac_toe" >
        </activity>
        <activity
            android:name="com.nguyentiendung.mediaplayer.MainActivity1"
            android:label="@string/title_activity_main_activity1" >
        </activity>
        <activity android:label="Online Music" android:name="com.nguyentiendung.online.PlayOnlineActivity">
            <intent-filter></intent-filter>
        </activity>
    </application>

</manifest>